{
  "name": "Playback Time Calculator(YouTube)",
  "description": "Calculate time by playback speed of video and playlist.",
  "version": "1.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://*.udemy.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "/assets/icon_128.png"
  }
}